{
  "name": "personal-site",
  "homepage": "https://the.delchop.io",
  "description": "This is the application that will function as my personal website",
  "version": "0.0.1",
  "main": "server/index.js",
  "keywords": ["react", "redux", "webpack"],
  "repository": "https://github.com/thedelchop/personal-site",
  "author": "Joseph DelCioppio <joseph.delcioppio@gmail> (https://the.delchop.io/)",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thedelchop/personal-site/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "yarn clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "start:prod": "yarn build && yarn serve",
    "serve": "cross-env NODE_ENV=production node buildServer/index.js",
    "build": "yarn build:client && yarn build:server && yarn build:node",
    "build:client":
      "rimraf buildClient && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server":
      "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server/index.js -o buildServer/index.js",
    "clean": "rimraf buildClient buildServer",
    "lint": "eslint_d --fix src server webpack",
    "format": "prettier --write '{src,server,webpack}/**/*.js' && yarn run lint"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "express": "^4.15.2",
    "history": "^4.6.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-universal-component": "^2.5.0",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.15",
    "redux-first-router-link": "^1.1.4",
    "webpack-flush-chunks": "^1.1.22"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.4",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-universal-import": "^1.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-watch": "^2.0.6",
    "bluebird": "^3.5.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-css-chunks-webpack-plugin": "^2.0.15",
    "prettier": "^1.13.6",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.6.1",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.2.0",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.1.0"
  }
}
